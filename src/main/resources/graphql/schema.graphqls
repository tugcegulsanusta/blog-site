type Post{
    id: ID!,
    header: String!,
    content: String!,
    base64img: String,
    category: Category!
}
type Category{
    id: ID!,
    name: String!
}
schema {
    query: Query
    mutation: Mutation
}
type Query {
    getPostsByHeader(type: String): [Post]
}
type Mutation {
    createPost(header: String!, content: String!, base64img: String, categoryName: String): Post!
    createCategory(name: String): Category!

}

